{
  "version": 2,
  "builds": [
    { "src": "api/upload.js", "use": "@vercel/node" },
    { "src": "main.py", "use": "@vercel/python" },
    { "src": "static/", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/upload", "dest": "/api/upload" },
    { "src": "/api/(.*)", "dest": "/main.py" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/static/index.html" }
  ]
}
