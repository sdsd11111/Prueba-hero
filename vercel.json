{
  "version": 2,
  "builds": [
    { "src": "api/upload.js", "use": "@vercel/node" },
    { "src": "src/routes/platos.py", "use": "@vercel/python" },
    { "src": "static/", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/upload", "dest": "/api/upload" },
    { "src": "/api/platos", "dest": "/src/routes/platos.py" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": "^/(.*)", "dest": "/static/index.html" }
  ]
}
